{
  "name": "FB Feed Modifier",
  "version": "1.1.0",
  "description": "Removes distracting posts from Facebook news feed. You can also view GRE words with meanings (from Barron 333 high frequency words) instead of an empty feed.",
  "content_scripts": [{
      "matches": ["*://*.facebook.com/*"],
      "js": ["js/jquery-3.1.0.min.js", "kill.js"],
      "run_at": "document_start"
  }],
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/gre-icon.png",
      "38": "images/gre-icon.png"
    },
    "default_title": "FB Feed Modifier",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/gre-icon.png",
    "48": "images/gre-icon.png",
    "128": "images/gre-icon.png"
  },
  "manifest_version": 2
}
